<h1>THE CONTACT PAGE</h1>
    <form action="/contact" method="get">
        <input type="text" name="member" value="<%= params[:member] %>" placeholder="Who are you looking for?">
        <input type="submit">
    </form>

<ul>
    <% @members.each do |member| %>
    <li>
        <%= member.capitalize %>
    </li>
    <% end %>
</ul>
    <%= link_to "HOME", root_path %>



    